{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    { "type": "panel", "id": "timeseries", "name": "Time series", "version": "8.0.0" },
    { "type": "panel", "id": "stat", "name": "Stat", "version": "8.0.0" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "1.0.0" }
  ],
  "annotations": { "list": [] },
  "editable": true,
  "panels": [
    {
      "type": "stat",
      "title": "Qdrant Up",
      "gridPos": { "x": 0, "y": 0, "w": 6, "h": 4 },
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        { "expr": "max(up{job=\"qdrant\"})", "legendFormat": "up" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Prometheus scrape duration (qdrant)",
      "gridPos": { "x": 6, "y": 0, "w": 9, "h": 8 },
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        { "expr": "avg by (job) (scrape_duration_seconds{job=\"qdrant\"})", "legendFormat": "scrape" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Qdrant metrics inventory (top names)",
      "gridPos": { "x": 15, "y": 0, "w": 9, "h": 8 },
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        {
          "expr": "topk(10, count by(__name__) ({__name__=~\"qdrant_.*\"}))",
          "legendFormat": "{{__name__}}"
        }
      ],
      "description": "Bu panel, mevcut Qdrant metriklerinin isimlerini keşfetmek için hızlı bir görünüm sağlar. Üretim ortamında isimler Qdrant sürümüne göre değişebilir."
    },
    {
      "type": "timeseries",
      "title": "RAG search latency p95 (from API)",
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 },
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, collection) (rate(rag_search_seconds_bucket[5m])))",
          "legendFormat": "{{collection}} p95"
        }
      ],
      "description": "Uygulamadan ölçülen Qdrant arama gecikmeleri. Qdrant iç metrikleri ile birlikte takip edilmesi önerilir."
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["qdrant", "vector", "overview"],
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "query": "prometheus",
        "current": { "text": "Prometheus", "value": "Prometheus" }
      }
    ]
  },
  "time": { "from": "now-6h", "to": "now" },
  "timezone": "browser",
  "title": "Qdrant – Overview",
  "version": 1
}

